{
	"name": "nouvel amour",
	"manifest_version": 2,
	"description": "GrinnellPlans NewLove mk 2",
	"version": "1.99.1",
	"permissions": [
		"http://*.grinnellplans.com/",
		"https://*.grinnellplans.com/",
		"storage",
		"notifications"],
	"content_scripts": [{
		"matches": ["*://*.grinnellplans.com/search.php?mysearch=*&planlove=1"],
		"js": ["jquery.min.js", "content_script.js"]
	}],
	"background": {
		"scripts": ["upgrade.js", "background.js"]
	},
	"options_page": "options.html",
	"web_accessible_resources": ["love-notify.png", "alert.png"]
}
